{
  "sources": [
    {
      "source_kind": "RADIO",
      "source_name": "Rock Antenne",
      "exclude" : {
        "regexp": "([\\S ]+) - ([\\S ]+)",
        "mapping": [
          {
            "matching_group": 1,
            "if_equals": "ROCK ANTENNE"
          }
        ]
      },
      "dissect": {
        "regexp": "([\\S ]+) - ([\\S ]+)",
        "mapping" : [
          {
            "matching_group": 2,
            "field": "title"
          },
          {
            "matching_group": 1,
            "field": "artist"
          }
        ]
      }
    },

    {
      "source_kind": "BLUETOOTH"
    }
  ]
}